    "start": "next start",
    "lint": "next lint",
    "build": "next build",
    "test": "jest --coverage",
    "pretty": "prettier --write --config .prettierrc.json {src,pages,server}/**/**/*.{ts,tsx}",
    "prettier": "prettier '**/*.{ts,tsx}'",
